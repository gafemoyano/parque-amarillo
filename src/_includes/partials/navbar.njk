    </button>

  </nav>
  <!--Menu-->
  <div
      class="w-full text-center flex border-b"
      :class="{ 'block shadow-3xl': isOpen, 'hidden': !isOpen }"
      x-show.transition="true"
    >
    {% if navigation.items %}
      <ul
        class="items-center justify-end flex-1 py-4  list-none px-4"
      >
        {% for item in navigation.items %}
          <li>
            <a
                @click="isOpen = false"
                class="inline-block px-4 py-2 text-sm font-semibold transition-all duration-150 ease-in text-body hover:text-accent focus:text-accent w-full "
                href="{{ item.url }}">
              {{ item.text }}
            </a>
          </li>
        {% endfor %}
      </ul>
    {% endif %}
  </div>
</div>